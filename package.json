{
  "name": "mtp-nodejs-port",
  "productName": "MTP Kernel for Node.js - Native addon",
  "version": "1.0.0",
  "description": "MTP Kernel for Node.js - Native addon",
  "main": "src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node --inspect --trace-warnings -r @babel/register src/index.js",
    "dev-rebuild": "cross-env NODE_ENV=development yarn native-build-dev && yarn dev",
    "native-build-dev": "autogypi && node-gyp rebuild --debug configure",
    "native-build-prod": "node-gyp rebuild",
    "native-clean": "node-gyp clean",
    "package-macos": "pkg dist/index.js --options expose-gc --target node10-macos-x64",
    "bundle-js": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.base.js",
    "export": "yarn native-build-prod && yarn bundle-js && yarn package-macos",
    "lint": "cross-env NODE_ENV=development eslint --cache --format=pretty --ext .js .",
    "lint-fix": "yarn --silent lint --fix; exit 0",
    "postlint-fix": "prettier --ignore-path .eslintignore --single-quote --write '**/*.*{js,json}'"
  },
  "keywords": [
    "openmtp",
    "kernel",
    "mtp",
    "nodejs",
    "port"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ganeshrvel/mtp-nodejs-port.git"
  },
  "author": {
    "name": "Ganesh Rathinavel",
    "email": "ganeshrvel@outlook.com",
    "url": "https://www.linkedin.com/in/ganeshrvel/"
  },
  "license": "MIT",
  "gypfile": true,
  "dependencies": {
    "autogypi": "^0.2.2",
    "dayjs": "^1.8.19",
    "junk": "^2.1.0",
    "lodash.find": "^4.6.0",
    "mkdirp": "^0.5.1",
    "nbind": "^0.3.6",
    "node-gyp": "^3.3.1",
    "path-parse": "^1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-do-expressions": "^7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-dev-expression": "^0.2.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.5.0",
    "eslint-formatter-pretty": "^2.1.0",
    "eslint-plugin-compat": "^2.6.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "prettier": "^1.16.4",
    "prettier-eslint": "^8.8.2",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.3",
    "yarn": "^1.9.2"
  }
}
